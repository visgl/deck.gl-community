{
  "name": "@deck.gl-community/graph-layers",
  "version": "9.2.0-beta.9",
  "description": "WebGL2-Powered library for Graph Visualization",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "graph",
    "visualization",
    "force-directed layout",
    "gpu",
    "deck.gl"
  ],
  "type": "module",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./graph-style-schema.json": "./dist/graph-style-schema.json",
    "./graph-style-schema.cdn": {
      "types": "./dist/graph-style-schema.cdn.d.ts",
      "default": "./dist/graph-style-schema.cdn.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "node ./scripts/generate-graphstyle-schema.mjs",
    "build:schema": "node ./scripts/generate-graphstyle-schema.mjs",
    "test": "vitest run",
    "test-watch": "vitest"
  },
  "dependencies": {
    "@deck.gl/core": "~9.2.1",
    "@deck.gl/layers": "~9.2.1",
    "@deck.gl/widgets": "~9.2.1",
    "@loaders.gl/core": "^4.2.0",
    "@luma.gl/core": "~9.2.0",
    "@luma.gl/engine": "~9.2.0",
    "@luma.gl/shadertools": "~9.2.0",
    "@probe.gl/log": "^4.0.4",
    "apache-arrow": "^17.0.0",
    "cardinal-spline-js": "^2.3.10",
    "color": "^4.2.3",
    "core-js": "^3.29.0",
    "d3-array": "^3.2.2",
    "d3-dag": "^1.1.0",
    "d3-force": "^3.0.0",
    "d3-format": "^3.1.0",
    "d3-scale": "^4.0.2",
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "ngraph.generators": "^20.1.0",
    "zod": "^4.0.0",
    "zod-to-json-schema": "patch:zod-to-json-schema@npm%3A3.24.6#~/.yarn/patches/zod-to-json-schema-npm-3.24.6-1ea8d4e085.patch"
  },
  "peerDependencies": {
    "zod": "^4.0.0"
  }
}
