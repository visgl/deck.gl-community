{
  "name": "@deck.gl-community/leaflet",
  "description": "deck.gl integration with Leaflet",
  "license": "MIT",
  "version": "9.2.0-beta.10",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "webgl",
    "visualization",
    "leaflet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/deck.gl-community"
  },
  "type": "module",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build-bundle": "npx ocular-bundle ./src/bundle.ts --output=dist/dist.min.js",
    "build": "npm run build-bundle",
    "prepublishOnly": "npm run build-bundle",
    "test": "vitest run",
    "test-watch": "vitest"
  },
  "dependencies": {
    "@deck.gl/core": "~9.2.8"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.18",
    "leaflet": "^1.9.4"
  },
  "peerDependencies": {
    "leaflet": "^1.9.4"
  }
}
