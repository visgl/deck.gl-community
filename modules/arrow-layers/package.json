  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "vitest run",
    "test-watch": "vitest"
  },
  "dependencies": {
    "@deck.gl/aggregation-layers": "~9.2.8",
    "@deck.gl/core": "~9.2.8",
    "@deck.gl/geo-layers": "~9.2.8",
    "@deck.gl/layers": "~9.2.8",
    "@geoarrow/geoarrow-js": "^0.3.0",
    "@math.gl/core": "^4.0.0",
    "@math.gl/polygon": "^4.0.0",
    "apache-arrow": ">=15",
    "threads": "^1.7.0"
  }
