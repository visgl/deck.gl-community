{
  "name": "@deck.gl-community/arrow-layers",
  "version": "9.2.1",
  "description": "A suite of Apache Arrow enabled layers for deck.gl",
  "license": "MIT",
  "private": true,
  "keywords": [
    "deck.gl",
    "webgl",
    "visualization",
    "arrow",
    "geoarrow"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/deck.gl-community"
  },
  "type": "module",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
